{
  "name": "vibe-dashboard",
  "version": "1.0.0",
  "description": "Dashboard for managing Vibe Coding Lab instances on AWS",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "tsx watch server/index.ts",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "tsx server/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.972.0",
    "@aws-sdk/client-ec2": "^3.700.0",
    "@aws-sdk/client-ecr": "^3.971.0",
    "@aws-sdk/client-ecs": "^3.700.0",
    "@aws-sdk/client-elastic-load-balancing-v2": "^3.700.0",
    "@aws-sdk/client-iam": "^3.971.0",
    "@aws-sdk/client-sts": "^3.971.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "nanoid": "^5.0.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "concurrently": "^9.1.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
